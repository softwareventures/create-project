  "scripts": {
    "build": "webpack --env.production",
    "prepare": "tsc",
    "start": "webpack-dev-server --open",
    "test": "ava",
    "lint": "eslint . && prettier --check .",
    "fix": "eslint . --fix && prettier --write .",
    "semantic-release": "semantic-release"
  },
  "devDependencies": {
    "@softwareventures/eslint-config": "3.4.1",
    "@softwareventures/prettier-config": "1.0.2",
    "@softwareventures/semantic-release-config": "1.1.1",
    "@softwareventures/tsconfig": "2.3.2",
    "@softwareventures/webpack-config": "3.0.1",
    "ava": "3.15.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.17.0",
    "prettier": "2.2.1",
    "semantic-release": "17.3.2",
    "ts-loader": "8.0.14",
    "ts-node": "9.1.1",
    "typescript": "4.1.3",
    "webpack": "4.46.0",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.2"
  },
  "dependencies": {
    "@types/webpack-env": "1.16.0",
    "tslib": "2.1.0"
  },
  "eslintConfig": {
    "root": true,
    "extends": "@softwareventures"
  },
  "prettier": "@softwareventures/prettier-config",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register/files"
    ]
