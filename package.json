{
  "name": "@softwareventures/create-project",
  "version": "0.0.0-development",
  "description": "Create a TypeScript project with standard settings for Software Ventures Limited",
  "keywords": [
    "create-project",
    "typescript",
    "template"
  ],
  "author": "Daniel Cassidy <mail@danielcassidy.me.uk>",
  "homepage": "https://github.com/softwareventures/create-project",
  "bugs": "https://github.com/softwareventures/create-project/issues",
  "repository": "github:softwareventures/create-project",
  "license": "ISC",
  "scripts": {
    "prepare": "shx rm -rf template/idea.template && shx cp -r .idea template/idea.template && tsc",
    "test": "tslint --project . && ava",
    "semantic-release": "semantic-release"
  },
  "sideEffects": [
    "./index.js"
  ],
  "bin": {
    "create-project": "./index.js"
  },
  "dependencies": {
    "@softwareventures/array": "0.31.0",
    "@types/empty-dir": "2.0.0",
    "@types/jsdom": "16.2.1",
    "@types/node": "12.12.36",
    "@types/prettier-package-json": "2.1.0",
    "@types/recursive-readdir": "2.2.0",
    "@types/xml-formatter": "1.1.0",
    "empty-dir": "2.0.0",
    "jsdom": "16.2.2",
    "non-null": "2.0.1",
    "prettier-package-json": "2.1.3",
    "recursive-readdir": "2.2.2",
    "tslib": "1.11.1",
    "xml-formatter": "2.0.0",
    "yarn": "1.22.4"
  },
  "devDependencies": {
    "@softwareventures/tsconfig": "2.2.0",
    "@softwareventures/tslint-rules": "0.8.1",
    "ava": "3.7.1",
    "cz-conventional-changelog": "3.1.0",
    "semantic-release": "17.0.6",
    "shx": "0.3.2",
    "ts-node": "8.9.0",
    "tslint": "6.1.1",
    "typescript": "3.8.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
