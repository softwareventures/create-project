{
  "name": "@softwareventures/create-project",
  "version": "0.0.0-development",
  "description": "Create a TypeScript project with standard settings for Software Ventures Limited",
  "keywords": [
    "create-project",
    "typescript",
    "template"
  ],
  "author": "Daniel Cassidy <mail@danielcassidy.me.uk>",
  "homepage": "https://github.com/softwareventures/create-project",
  "bugs": "https://github.com/softwareventures/create-project/issues",
  "repository": "github:softwareventures/create-project",
  "license": "ISC",
  "scripts": {
    "prepare": "tsc",
    "test": "tslint --project . && ava",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@softwareventures/array": "^0.14.0",
    "@types/node": "^12.0.2",
    "@types/prettier-package-json": "^2.1.0",
    "empty-dir": "^2.0.0",
    "prettier-package-json": "^2.1.0"
  },
  "devDependencies": {
    "@softwareventures/tsconfig": "2.0.0",
    "@softwareventures/tslint-rules": "0.6.6",
    "ava": "1.4.1",
    "cz-conventional-changelog": "2.1.0",
    "semantic-release": "15.13.12",
    "ts-node": "8.2.0",
    "tslint": "5.17.0",
    "typescript": "3.5.1"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
